<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="320dp"
    android:layout_height="395dp"
    android:layout_gravity="center"
    android:background="@color/white">

    <Button
        android:id="@+id/splashPlay"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/cyan"
        android:soundEffectsEnabled="false"
        android:text="PLAY!"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/splashHighScores"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/splashHighScores"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="24dp"
        android:background="@color/cyan"
        android:soundEffectsEnabled="false"
        android:text="High Scores"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/splashOptions"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/splashOptions"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:layout_alignBottom="@+id/splash"
